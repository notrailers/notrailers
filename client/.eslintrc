{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "env": {
      "browser": true,
      "node": true
  },
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  },
  "rules": {
      "jsx-quotes": 0,
      "no-console": 0,
      "no-unused-vars": 1,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "semi": 0
  }
}
